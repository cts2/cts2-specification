  </xs:complexType>
  
  <xs:element name="existsMapVersionForMap" type="tns:existsMapVersionForMap"/>
  <xs:complexType name="existsMapVersionForMap">
    <xs:sequence>
      <xs:element minOccurs="1" name="map" type="coreService:NameOrURI"/>
