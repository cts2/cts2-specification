<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:core="http://www.omg.org/spec/CTS2/1.2/Core"
  xmlns:map="http://www.omg.org/spec/CTS2/1.2/MapCatalog"
  xmlns:mapVersion="http://www.omg.org/spec/CTS2/1.2/MapVersion"
