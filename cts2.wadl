              </representation>

            </response>

            <response status="404">
              <representation element="exceptions:UnknownStatement">
                <doc xmlns="http://www.w3.org/2001/XMLSchema">The referenced statement is not
                recognized by the service</doc>

              </representation>

            </response>

          </method>

        </resource>

        <resource path="latestchange">
