        <method name="GET">
          <doc title="StatementService:getStatement" />
          <request>
            <param name="active" type="xsd:string" style="query" default="">
              <doc title="ReadContext:active">determines whether the query only applies to only
              active or all entries.</doc>

            </param>

            <param name="changesetcontext" type="xsd:string" style="query" default="">
              <doc title="ReadContext:changeSetContext">the URI of an open change set whose
